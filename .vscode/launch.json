{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--input_folder=dict",
                "--output_folder=output",
                "--filter=Tu_dien_Han_ngu"
            ]
        }
    ]
}
